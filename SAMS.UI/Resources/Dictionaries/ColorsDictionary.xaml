<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="Color_White">#FCFCFD</Color>
    <Color x:Key="Color_Black">#1E1E1E</Color>
    <Color x:Key="Color_Gold">#FFD500</Color>
    <Color x:Key="Color_MikadoYellow">#FDC500</Color>
    <Color x:Key="Color_PolynesianBlue">#00509D</Color>
    <Color x:Key="Color_MarianBlue">#003F88</Color>
    <Color x:Key="Color_RoyalBlue">#00296B</Color>
    <Color x:Key="Color_RustyRed">#D52C40</Color>

    <SolidColorBrush x:Key="SolidColorBrush_White" Color="{StaticResource Color_White}" />
    <SolidColorBrush x:Key="SolidColorBrush_Black" Color="{StaticResource Color_Black}" />
    <SolidColorBrush x:Key="SolidColorBrush_Gold" Color="{StaticResource Color_Gold}" />
    <SolidColorBrush x:Key="SolidColorBrush_MikadoYellow" Color="{StaticResource Color_MikadoYellow}" />
    <SolidColorBrush x:Key="SolidColorBrush_PolynesianBlue" Color="{StaticResource Color_PolynesianBlue}" />
    <SolidColorBrush x:Key="SolidColorBrush_MarianBlue" Color="{StaticResource Color_MarianBlue}" />
    <SolidColorBrush x:Key="SolidColorBrush_RoyalBlue" Color="{StaticResource Color_RoyalBlue}" />
    <SolidColorBrush x:Key="SolidColorBrush_RustyRed" Color="{StaticResource Color_RustyRed}" />

</ResourceDictionary>